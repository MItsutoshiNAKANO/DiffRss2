    "yargs": "^17.7.2"
  },
  "main": "src/functions/*.js",
  "devDependencies": {
    "standard": ">=17.1.0"
  },
  "IWillTestBy": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.7.0",
    "expect": "^29.7.0",
    "jest": "^29.7.0"
  }
}
